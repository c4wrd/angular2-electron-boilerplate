{
  "name": "angular2-boilerplate",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "scripts": {
    "build": "gulp frontend && tsc",
    "start": "npm run build && concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run sasswatch\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "sasswatch": "gulp sass:watch",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.1",
    "lite-server": "^2.2.0",
    "run-sequence": "^1.1.5",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "keywords": ["Angular", "Angular2", "typescript"],
  "author": "Cory Forward",
  "license": "MIT",
  "description": "An easy to use Angular 2 boilerplate for Angular 2 RC1"
}
